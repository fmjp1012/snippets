{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

    "report": {
        "prefix": "report",
        "body": [
            "\\documentclass[dvipdfmx]{jsarticle}",
            "",
            "\\usepackage{amsmath}",
            "\\usepackage[dvipdfmx]{graphicx}",
            "\\usepackage{pdfpages}",
            "\\usepackage{showlabels}",
            "\\usepackage{multirow}",
            "",
            "\\usepackage{textgreek}",
            "\\usepackage{ulem} % 取り消し線に使用する",
            "",
            "\\begin{document}",
            "% \\includepdf{cover.pdf}",
            "%----------------------------------------------------------------------------------------",
            "\\setcounter{page}{1}",
            "\\section{目的}",
            "%----------------------------------------------------------------------------------------",
            "\\section{実験原理}",
            "%----------------------------------------------------------------------------------------",
            "\\newpage",
            "\\section{実験方法}",
            "%----------------------------------------------------------------------------------------",
            "\\newpage",
            "\\section{実験結果}",
            "%----------------------------------------------------------------------------------------",
            "\\newpage",
            "\\section{考察}",
            "%----------------------------------------------------------------------------------------",
            "\\section{結論・まとめ}",
            "%----------------------------------------------------------------------------------------",
            "\\begin{thebibliography}{99}",
            "  \\bibitem{Text} 過渡応答 実験テキスト.",
            "  \\bibitem{Takahashi} 高橋清, 山田陽一. 半導体工学. 森北出版, 2013.",
            "\\end{thebibliography}",
            "%----------------------------------------------------------------------------------------",
            "\\end{document}"
        ],
        "description": "実験レポート"
    },
    "align": {
        "prefix": "align",
        "body": [
            "%",
            "\\begin{align}",
            "  $1\\notag \\\\\\",
            "  &= \\label{eq:$2}",
            "\\end{align}",
            "%"
        ],
        "description": "\\begin{align}"
    },
    "equation": {
        "prefix": "eq",
        "body": [
            "%",
            "\\begin{equation}",
            "  \\label{eq:$1}",
            "\\end{equation}",
            "%"
        ],
        "description": "\\begin{equation}"
    },
    "table": {
        "prefix": "table",
        "body": [
            "%",
            "\\begin{table}[htbp]",
            "\\caption{$1}",
            "\\label{tab:$2}",
            "\\centering",
            "  \\begin{tabular}{cccc}",
            "  \\hline",
            "  a&b&c&d\\\\\\",
            "  \\hline\\hline",
            "  e&f&g&h\\\\\\",
            "  \\hline",
            "\\end{tabular}",
            "\\end{table}",
            "%"
        ]
    },
    "figure": {
        "prefix": "figure",
        "body": [
            "%",
            "\\begin{figure}[htbp]",
            "  \\centering",
            "  \\includegraphics[width=10cm,pagebox=cropbox,clip]{$1}",
            "  \\caption{$2}",
            "  \\label{fig:$3}",
            "\\end{figure}",
            "%"
        ]
    },
    "space": {
        "prefix": "space",
        "body": [
            "\\,$1"
        ]
    },
    "si": {
        "prefix": "si",
        "body": [
            "\\SI{$1}{$2}"
        ]
    },
    "do": {
        "prefix": "do",
        "body": [
            "${$1}^\\circ$"
        ]
    },
    "red": {
        "prefix": "red",
        "body": [
            "\\color{red}$1\\color{black}"
        ]
    }
}